# MCP Gateway Demo Environment Variables
# Copy this file to .env and fill in your values

# GitHub Personal Access Token (for GitHub MCP server)
GITHUB_TOKEN=your_github_personal_access_token_here

# OpenAI API Key (for PR recommender)
OPENAI_API_KEY=your_openai_api_key_here

# Memory Plugin Token (for memory server)
MEMORY_PLUGIN_TOKEN=default-memory-token-for-testing

# MCP Gateway Secret (for JWT generation)
MCP_GATEWAY_SECRET=your-secret-key-here

# MCP Gateway Admin Credentials
MCP_GATEWAY_ADMIN_USER=admin
MCP_GATEWAY_ADMIN_PASSWORD=admin123

# Database Credentials (PostgreSQL)
POSTGRES_DB=mcp_gateway
POSTGRES_USER=mcp_user
POSTGRES_PASSWORD=mcp_password

# Redis Configuration
REDIS_PASSWORD=your_redis_password_here
